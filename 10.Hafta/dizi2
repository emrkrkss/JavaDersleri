package dizi120222;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		 
		 
	        int[] dizi = {10,2,3,6,9,8,4,5};
	        for (int i = 0; i < dizi.length; i++) {
	        	for(int k=0; k<dizi[i];k++) {
	        		System.out.print("*");
	        		
	        	}
	        	System.out.println();
	        	
	        }
	       
			
		}
	}

