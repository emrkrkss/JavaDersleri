//Dizide kaÃ§ adet 1 var
package _231222;

public class main {

	public static void main(String[] args) {
		int a[][] ={{0,2,3},{5,5,0},{8,4,0},{6,8,0}};
		int b[][] ={{0,2,5},{5,1,0},{9,4,0},{6,8,1}};
		int sayac = 0;
		for(int i=0; i<4;i++)
		{
			for(int k=0; k<b[k].length;k++)
			{
				if(b[i][k]==1)
				{
					sayac++;
				}
				
				System.out.print(b[i][k]+"\t");
			}

			System.out.print("\n");
		}
		System.out.println(sayac+" Tane 1 var");	
	}
}
